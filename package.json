{
  "private": true,
  "name": "resume",
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && touch docs/.nojekyll",
    "fmt": "pnpify prettier --cache --write 'app/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' '*.{json,js}' next.config.mjs",
    "lint": "next lint && tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@types/node": "^20.4.4",
    "@types/react": "18.2.15",
    "@yarnpkg/pnpify": "^4.0.0-rc.48",
    "autoprefixer": "10.4.14",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "next": "13.4.12",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "@next/next/no-img-element": "off",
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "fixme"
          ]
        }
      ]
    }
  },
  "packageManager": "yarn@3.6.1"
}
