{
  "name": "resume",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs && touch docs/.nojekyll",
    "fmt": "prettier --write 'pages/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' '*.{json,js}' next.config.mjs",
    "lint": "next lint && tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@types/node": "^18.0.0",
    "@types/react": "18.0.14",
    "autoprefixer": "10.4.7",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "next": "12.1.6",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "tailwindcss": "3.1.4",
    "typescript": "4.6.4"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "@next/next/no-img-element": "off",
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "fixme"
          ]
        }
      ]
    }
  },
  "packageManager": "yarn@3.2.0"
}
