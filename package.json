{
  "private": true,
  "name": "resume",
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs && touch docs/.nojekyll",
    "fmt": "prettier --cache --write 'pages/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' '*.{json,js}' next.config.mjs",
    "lint": "next lint && tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@types/node": "^20.3.3",
    "@types/react": "18.2.14",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  },
  "resolutions": {
    "json5": "^2.2.2"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "@next/next/no-img-element": "off",
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "fixme"
          ]
        }
      ]
    }
  },
  "packageManager": "yarn@3.6.1"
}
