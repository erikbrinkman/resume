{
  "private": true,
  "name": "resume",
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs && touch docs/.nojekyll",
    "fmt": "prettier --cache --write 'pages/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' '*.{json,js}' next.config.mjs",
    "lint": "next lint && tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@types/node": "^18.11.9",
    "@types/react": "18.0.25",
    "autoprefixer": "10.4.13",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "next": "13.0.5",
    "postcss": "8.4.19",
    "prettier": "2.8.0",
    "prettier-plugin-organize-imports": "^3.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "minimatch": "^3.0.5"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "@next/next/no-img-element": "off",
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "fixme"
          ]
        }
      ]
    }
  },
  "packageManager": "yarn@3.3.0"
}
