{
  "name": "resume",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs && touch docs/.nojekyll",
    "fmt": "prettier --write 'pages/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' '*.{json,js}' next.config.mjs",
    "lint": "next lint && tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@types/node": "^17.0.31",
    "@types/react": "18.0.9",
    "autoprefixer": "10.4.7",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "next": "12.1.6",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.3.1",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "@next/next/no-img-element": "off",
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "fixme"
          ]
        }
      ]
    }
  },
  "packageManager": "yarn@3.2.0"
}
