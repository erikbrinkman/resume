{
  "name": "resume",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs && touch docs/.nojekyll",
    "format": "prettier --write 'pages/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' '*.{json,js,ts}'",
    "lint": "next lint && tsc"
  },
  "devDependencies": {
    "@types/react": "17.0.11",
    "autoprefixer": "10.2.6",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "next": "11.0.1",
    "postcss": "8.3.5",
    "prettier": "2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "tailwindcss": "2.2.4",
    "typescript": "4.3.4"
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "@next/next/no-img-element": "off",
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "fixme"
          ]
        }
      ]
    }
  }
}
